{
  "name": "deer-ui",
  "version": "0.0.1",
  "description": "React.js UI components for PC Web",
  "main": "dist/index.js",
  "module": "es/index.js",
  "scripts": {
    "dev": "webpack-dev-server --config scripts/webpack.dev.config.js --progress",
    "clean": "rimraf dist lib es",
    "styleguidist": "styleguidist server",
    "styleguidist:build": "styleguidist build",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "storybook": "start-storybook -p 8080",
    "build:types": "rimraf types && tsc --outDir types -d --emitDeclarationOnly",
    "build:css": "webpack --config scripts/webpack.css.config.js --progress",
    "build:umd": "webpack --config scripts/webpack.prod.config.js --progress",
    "build:es": "babel src -d es --ignore **/__tests__",
    "build:lib": "cross-env OUTPUT_MODULE=commonjs babel src/components -d lib --ignore **/__tests__",
    "build": "npm run clean && npm run build:umd && npm run build:es && npm run build:lib && npm run build:css && rimraf ignore"
  },
  "files": [
    "lib",
    "es",
    "dist",
    "LICENSE"
  ],
  "author": "boyang",
  "license": "MIT",
  "keywords": [
    "deer",
    "deer-ui",
    "react",
    "tool",
    "react-component",
    "component",
    "components",
    "ui",
    "web",
    "pc-component",
    "framework",
    "frontend"
  ],
  "tags": [
    "deer",
    "deer-ui",
    "react",
    "react-ui",
    "component",
    "components",
    "javascript",
    "es6",
    "css3"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addon-notes": "^5.2.5",
    "@storybook/addon-options": "^5.2.5",
    "@storybook/addon-storysource": "^5.2.5",
    "@storybook/addon-viewport": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@storybook/storybook-deployer": "^2.8.1",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "docz": "^2.0.0-rc.70",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^1.2.0",
    "highlight": "^0.2.4",
    "highlight.js": "^9.16.2",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "react-icons": "^3.8.0",
    "react-lowlight": "^2.0.0",
    "react-markdown": "^4.2.2",
    "react-styleguidist": "^10.2.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
